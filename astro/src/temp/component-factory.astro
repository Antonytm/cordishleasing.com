---
  
import BrandStory from '../../src/components/BrandStory.astro'
import ContentBlock from '../../src/components/ContentBlock.astro'
import Footer from '../../src/components/Footer.astro'
import GenericContentFeature from '../../src/components/GenericContentFeature.astro'
import Header from '../../src/components/Header.astro'
import HeroSlider from '../../src/components/HeroSlider.astro'
import Placeholder from '../../src/components/system/Placeholder.astro'

export interface Props  {
  name: string;
  route: any;
}
---

<>
{Astro.props.name === 'BrandStory' && <BrandStory route={Astro.props.route}/>}
{Astro.props.name === 'ContentBlock' && <ContentBlock route={Astro.props.route}/>}
{Astro.props.name === 'Footer' && <Footer route={Astro.props.route}/>}
{Astro.props.name === 'GenericContentFeature' && <GenericContentFeature route={Astro.props.route}/>}
{Astro.props.name === 'Header' && <Header route={Astro.props.route}/>}
{Astro.props.name === 'HeroSlider' && <HeroSlider route={Astro.props.route}/>}
{Astro.props.name === 'Placeholder' && <Placeholder route={Astro.props.route}/>}
  {Astro.props.name === undefined && Astro.props.route.name === 'code' && <code {...Astro.props.route.attributes} set:html={Astro.props.route.contents}></code>}
</>
